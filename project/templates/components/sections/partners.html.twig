<section class="section section_light partners">
    <div class="container">
        <div class="section__header">
            <h2 class="section__heading">
                Партнеры
            </h2>
        </div>
        <div class="swiper partners__swiper partner-swiper">
            <ul class="swiper-wrapper partners-swiper__wrapper">
                {% for i in 1..8 %}
                    <li class="swiper-slide partners-swiper__slide">
                        <img src="{{ asset("build/images/partner-#{i}.svg") }}" alt="partner">
                    </li>
                {% endfor %}
            </ul>
        </div>
    </div>
</section>